{
  "name": "ffxiv-vfx-port",
  "version": "1.0.1",
  "description": "Electron app to port VFX from one Gear to another in FFXIV.",
  "author": "Shino Mythmaker",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --config electron-builder.json",
    "build:portable": "electron-builder --config electron-builder.json --win portable",
    "build:installer": "electron-builder --config electron-builder.json --win nsis"
  },
  "devDependencies": {
    "electron": "^30.0.0",
    "electron-builder": "^26.0.12"
  }
}